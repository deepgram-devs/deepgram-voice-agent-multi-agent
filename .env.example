  # Twilio Configuration
  TWILIO_ACCOUNT_SID=your_twilio_account_sid
  TWILIO_AUTH_TOKEN=your_twilio_auth_token
  TWILIO_PHONE_NUMBER=+1234567890

  # Phone Numbers
  LEAD_PHONE_NUMBER=+1234567890  # Number to call (must be verified for trial Twilio accounts)

  # Server Configuration
  LEAD_SERVER_HOST=0.0.0.0
  LEAD_SERVER_PORT=8000
  LEAD_SERVER_EXTERNAL_URL=wss://your-tunnel-url.zrok.io  # From zrok or ngrok

  # Deepgram
  DEEPGRAM_API_KEY=your_deepgram_api_key

  # Voice Models (Deepgram Aura 2)
  QUALIFIER_VOICE_MODEL=aura-2-mars-en
  ADVISOR_VOICE_MODEL=aura-2-thalia-en
  CLOSER_VOICE_MODEL=aura-2-vesta-en

  # Groq (for context summarization between agents)
  GROQ_API_KEY=your_groq_api_key  # Get free key at https://console.groq.com/
  GROQ_LLM=llama-3.3-70b-versatile  # LLM model for conversation summarization, see other available models here: https://console.groq.com/docs/models